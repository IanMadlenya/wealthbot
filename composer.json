{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.4.3",

        "symfony/symfony": "2.2.*",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.2.*",
        "symfony/monolog-bundle": "2.2.*",
        "symfony/security-bundle": "2.2.*",

        "sensio/distribution-bundle": "2.2.*",
        "sensio/framework-extra-bundle": "2.2.*",
        "sensio/generator-bundle": "2.2.*",

        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.2.*",

        "doctrine/data-fixtures": "*",
        "doctrine/doctrine-fixtures-bundle": "*",
        "doctrine/mongodb": "1.0.*@dev",
        "doctrine/mongodb-odm": "1.0.*@dev",
        "doctrine/mongodb-odm-bundle": "3.0.*@dev",

        "friendsofsymfony/user-bundle": "~1.3",
        "friendsofsymfony/jsrouting-bundle": "~1.1",

        "hpatoio/deploy-bundle": "*",

        "incenteev/composer-parameter-handler": "dev-master",

        "instaclick/base-test-bundle": "dev-master",

        "jms/security-extra-bundle": "1.4.*",
        "jms/di-extra-bundle": "1.3.*",
        "jms/serializer-bundle": "~0.13",

        "knplabs/knp-paginator-bundle": "dev-master",
        "knplabs/knp-snappy-bundle": "dev-master",

        "nelmio/api-doc-bundle": "2.1.0",

        "ornicar/apc-bundle": "1.0.*",

        "phpoffice/phpexcel": "1.7.8",

        "stof/doctrine-extensions-bundle": "dev-master",

        "twig/extensions": "1.0.*"

    },
    "repositories": [{
        "type": "package",
        "package": {
            "name": "PHPOffice/PHPExcel",
            "version": "1.7.8",
            "source": {
                "url": "https://github.com/PHPOffice/PHPExcel.git",
                "type": "git",
                "reference": "PHPExcel_1.7.8"
            },
            "autoload": {
                "psr-0": {
                    "PHPExcel": "Classes/"
                }
            }
        }
    }],
    "require-dev": {
        "phpunit/phpunit": "4.5.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 900
    },
    "minimum-stability": "dev",
    "extra": {
        "incenteev-parameters": [
            {
                "file": "app/config/parameters.yml",
                "keep-outdated": true
            },
            {
                "file": "app/config/parameters_test.yml",
                "keep-outdated": false
            }
        ],
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
